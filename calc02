public class Nums {

    double num1;
    double num2;


    double add;
    public void add(){
        add = num1 + num2;
    }


    double sub;
    public void sub(){
        sub = num1 - num2;
    }


    double mult;
    public void mult(){
        mult = num1 * num2;
    }


    double div;
    public void div(){
        div = num1 / num2;
    }
}
